                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=11.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">-- SQL exemplo Vido CPF recupera CNPJ

DECLARE @currentDate DATE
,		@dS2 AS VARCHAR(10)
,		@dP1 AS VARCHAR(10)
,		@dS2_file AS VARCHAR(8)
,		@d_file AS VARCHAR(8)
,		@randomString AS VARCHAR(6)
,		@prevDays	AS INTEGER

SET @prevDays = -2 
SET @randomString = lower((SELECT CONVERT(varchar(255), NEWID())))

SET @currentDate = GETDATE()

SET @dS2 = (SELECT CONVERT (varchar(10), dateadd(day,@prevDays,@currentDate), 103))
SET @dP1 = (SELECT CONVERT (varchar(10), dateadd(day,1,@currentDate), 103))

SET @dS2_file = (SELECT CONVERT (varchar(10), dateadd(day,@prevDays,@currentDate), 112))
SET @d_file = (SELECT CONVERT (varchar(10), @currentDate, 112))

--SELECT @dS2			AS date_sub2
--,	(SELECT CONVERT (varchar(10), @currentDate, 103))		AS date_current
--,	@dP1					AS date_plus1
--,	@dS2_file				AS nom_arquivo_inicial
--,	@d_file					AS nom_arquivo_final
--,	('ListaCPF_cnpj_COM_Contato_acessoPortalSebrae_de_' + @dS2_file +'a' + @d_file  + '_' + @randomString)	AS nom_arquivo_carga_sandbox
--,	('ListaCNPJ_COM_Contato_acessoPortalSebrae_de_' + @dS2_file +'a' + @d_file + '_' + @randomString)			AS nom_arquivo_a_ser_enviado
--,	@randomString			AS random_string


IF OBJECT_ID('DATAHUB_SANDBOX.ANALISEDADOSFAMPE.TempPortalAcessoCarga') IS NOT NULL
BEGIN
        DROP TABLE DATAHUB_SANDBOX.ANALISEDADOSFAMPE.TempPortalAcessoCarga
END

CREATE TABLE DATAHUB_SANDBOX.ANALISEDADOSFAMPE.TempPortalAcessoCarga
(
   CPF VARCHAR(50),
   CNPJ VARCHAR(50),
   cel_pessoal VARCHAR(50)
)

DECLARE @query nvarchar(max) = 
'
SELECT	DISTINCT CPF
,		CNPJ
,		cel_pessoal 
FROM OPENQUERY(ANALISEDADOSFAMPE,
    '' SELECT * FROM SISTAPOIOPORTAL.saportal.FN_LIST_USUARIOS_ACESSO_CTD(''''cb5b84175c481710VgnVCM1000004c00210aRCRD'''','''''+@dS2+''''','''''+@dP1+''''')''
)';

INSERT INTO DATAHUB_SANDBOX.ANALISEDADOSFAMPE.TempPortalAcessoCarga
EXEC(@query);

--SELECT COUNT(*) FROM #TempPortalAcessoCarga;

;WITH cte_acesso_portal AS (
    SELECT * FROM DATAHUB_SANDBOX.ANALISEDADOSFAMPE.TempPortalAcessoCarga
),	cte_CNPJ_contato AS (
	-- CTE_dos_CNPJs com acesso ao Video Portal
	SELECT CAST(CNPJ AS BIGINT) AS CNPJ_int
	,		cel_pessoal
	FROM (
		SELECT	DISTINCT CNPJ
		,		cel_pessoal
		FROM cte_acesso_portal
		WHERE 0 = 0
		AND CNPJ IS NOT NULL
		AND CNPJ NOT IN ('NULL')
	)tbCNPJFilled
)

SELECT	DISTINCT CNPJ_ref  AS CNPJ_int
,		cel_pessoal
FROM (
	SELECT	tbCpf.CPF
	,		tbCpf.CPF_INT
	,		SOCIOS.CodPessoaJuridicaCNPJ
	,		EMPRESAS.CNPJ
	,		CAST(COALESCE(SOCIOS.CodPessoaJuridicaCNPJ,EMPRESAS.CNPJ) AS BIGINT) AS CNPJ_ref
	,		cel_pessoal

	FROM (
		SELECT	DISTINCT CPF
		,		CPF_INT
		,		cel_pessoal 
		FROM (
			-- CPFs com acesso ao Video Portal	

			SELECT 	CPF
			,		CAST(CPF AS BIGINT) AS 	CPF_INT
			,		cel_pessoal
			FROM cte_acesso_portal
			WHERE 4 = 4
			AND CPF IS NOT NULL
			AND CPF NOT IN ('NULL')
		)tbA
	) tbCpf
	LEFT JOIN DH1MVP_HUB.dbo.VW011_SOCIOS_RFB SOCIOS
		ON CodPessoaFisica_int = CPF_INT
	LEFT JOIN DH1MVP_HUB.dbo.VW010_EMPRESAS_RFB EMPRESAS
			ON EMPRESAS.RESPONSAVEL_CPF = tbCpf.CPF
	WHERE 2 = 2
	AND	EMPRESAS.SIT_CADASTRAL IN('02')
) tbCnpjAtivo

UNION
SELECT	CNPJ_int  
,		cel_pessoal
FROM cte_CNPJ_contato</property>
